{
	"name": "sudobee/sparcel",
	"description": "Common library to simplify connection with Sparcel services",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "SudoBee",
			"email": "info@sudobee.com"
		}
	],
	"require": {
		"php": "^7.4|^8.0|^8.1",
		"ext-json": "*",
		"ext-curl": "*"
	},
	"require-dev": {
		"pestphp/pest": "^1.15"
	},
	"autoload": {
		"psr-4": {
			"SudoBee\\Sparcel\\": "src"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
